{
  "version": 2,
  "builds": [
    {
      "src": "music/wsgi.py",
      "use": "@vercel/python",
      "config": { "buildcommand": "build_files.sh" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "music/wsgi.py"
    }
  ]
}
