{
  "version": 2,
  "builds": [
    {
      "src": "music/wsgi.py",
      "use": "@vercel/python",
      "config": { "buildcommand": "build_files.sh" }
    },
      {
        "use": "@vercel/static"
      }
  ],
  "routes": [
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "music/wsgi.py"
    }
  ]
}
