{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "vercel_app.py"
      
    }
  ]
}
