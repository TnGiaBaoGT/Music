{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ]
}

