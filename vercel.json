{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "max-age=31536000, immutable",
        "Content-Type": "application/javascript"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ]
}

