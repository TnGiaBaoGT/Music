
{
  "version": 2,
  "builds": [
    {
      "src": "music/wsgi.py",
      "use": {"@vercel/python", "@vercel/static-build"}
      "config": {
        "buildCommand": "./build_files.sh" , "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "music/wsgi.py"
    }
  ]
}

