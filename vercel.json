{
  "version": 2,
  "builds": [
    {
      "src": "music/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./build_files.sh"
      }
    },
    {
      "src": "staticfiles_build/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "music/wsgi.py"
    }
  ]
}
